          "title": "* Zone of the lens"
        },
        "app": {
          "type": "string",
          "title": "* App lens"
        },
        "pod": {
          "type": "string",
          "title": "* Pod of the lens"
        },
        "containerName": {
          "type": "string",
          "title": "* Container of the lens"
        },
        "process": {
          "type": "string",
